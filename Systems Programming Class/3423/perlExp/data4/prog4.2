#!/usr/bin/perl -w

$n = 0;
while ($files = <ARGV> ) {
	
	@line = split (/:/, $files);
	$all[$n] ="$line[0]\t\t$line[4]";
	$n++;
}

@all = sort (@all);

$i = 0;
while ($i<$n){
	print "$all[$i]\n";
	$i++;
}

